<div class="motion-at-transavia">
  <h1>Motion at Transavia</h1>

  <section class="page-item motion-at-transavia__section">
    <h2 class="motion-at-transavia__sub-title">Default easings</h2>

    <div class="motion-at-transavia__motion-block" data-ease="in">Ease in</div>
    <div class="motion-at-transavia__motion-block" data-ease="out">Ease out</div>
    <div class="motion-at-transavia__motion-block" data-ease="in-out">Ease in out</div>
  </section>

  <section class="page-item motion-at-transavia__section">
    <h2 class="motion-at-transavia__sub-title">Default easings with overflow</h2>

    <div class="motion-at-transavia__motion-block" data-ease="in" data-overflow="true">Ease in</div>
    <div class="motion-at-transavia__motion-block" data-ease="out" data-overflow="true">Ease out</div>
    <div class="motion-at-transavia__motion-block" data-ease="in-out" data-overflow="true">Ease in out</div>
  </section>

  <section class="page-item motion-at-transavia__section">
    <h2 class="motion-at-transavia__sub-title">Bounce easings</h2>

    <div class="motion-at-transavia__motion-block" data-ease="bounce-in">Ease in</div>
    <div class="motion-at-transavia__motion-block" data-ease="bounce-out">Ease out</div>
    <div class="motion-at-transavia__motion-block" data-ease="bounce-in-out">Ease in out</div>
  </section>
</div>
